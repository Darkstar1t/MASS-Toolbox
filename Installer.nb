Cell["Run the following code to install the Toolbox", "Subsection"],

Cell["\<\
The following code will install the MASS Toolbox in \
$UserBaseDirectory/Applications\
\>", "Text"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"install", "[", 
    RowBox[{"path_String:", 
     RowBox[{"FileNameJoin", "[", 
      RowBox[{"{", 
       RowBox[{"$UserBaseDirectory", ",", "\"\<Applications\>\""}], "}"}], 
      "]"}]}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "configPath", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"SetDirectory", "[", 
       RowBox[{"NotebookDirectory", "[", "]"}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Quiet", "[", 
       RowBox[{
        RowBox[{"DeleteDirectory", "[", 
         RowBox[{
          RowBox[{"FileNameJoin", "[", 
           RowBox[{"{", 
            RowBox[{"path", ",", "\"\<Toolbox\>\""}], "}"}], "]"}], ",", 
          RowBox[{"DeleteContents", "\[Rule]", "True"}]}], "]"}], ",", 
        RowBox[{"{", 
         StyleBox[
          RowBox[{"DeleteDirectory", "::", "nodir"}], "MessageName"], "}"}]}],
        "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"CopyDirectory", "[", 
       RowBox[{"\"\<Toolbox\>\"", ",", 
        RowBox[{"FileNameJoin", "[", 
         RowBox[{"{", 
          RowBox[{"path", ",", "\"\<Toolbox\>\""}], "}"}], "]"}]}], "]"}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"Quiet", "[", 
       RowBox[{
        RowBox[{"DeleteDirectory", "[", 
         RowBox[{
          RowBox[{"FileNameJoin", "[", 
           RowBox[{"{", 
            RowBox[{"path", ",", "\"\<AutomaticUnits\>\""}], "}"}], "]"}], 
          ",", 
          RowBox[{"DeleteContents", "\[Rule]", "True"}]}], "]"}], ",", 
        RowBox[{"{", 
         StyleBox[
          RowBox[{"DeleteDirectory", "::", "nodir"}], "MessageName"], "}"}]}],
        "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"CopyDirectory", "[", 
       RowBox[{"\"\<Toolbox\>\"", ",", 
        RowBox[{"FileNameJoin", "[", 
         RowBox[{"{", 
          RowBox[{"path", ",", "\"\<AutomaticUnits\>\""}], "}"}], "]"}]}], 
       "]"}], ";"}]}], "\[IndentingNewLine]", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"install", "[", "]"}]}], "Input",
