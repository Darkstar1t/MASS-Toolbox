Cell["Run the following code to uninstall the Toolbox", "Subsection"],

Cell["\<\
The following code will determine the location of the currently installed \
MASS Toolbox and uninstall it\
\>", "Text"],

Cell[BoxData[{
 RowBox[{"<<", "Toolbox`"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"Toolbox", "::", "gitrepository"}], "=", 
   "\"\<Package found at `1` is a GIT repository and not a regular \
installation of the Toolbox. It will not be removed.\>\""}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"SetDirectory", "[", 
   RowBox[{"NotebookDirectory", "[", "]"}], "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"installationDirectory", "=", 
   RowBox[{"FileNameJoin", "[", 
    RowBox[{
     RowBox[{"FileNameSplit", "[", 
      RowBox[{"DirectoryName", "[", 
       RowBox[{"FindFile", "[", "\"\<Toolbox`\>\"", "]"}], "]"}], "]"}], "[", 
     
     RowBox[{"[", 
      RowBox[{";;", 
       RowBox[{"-", "2"}]}], "]"}], "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{"MemberQ", "[", 
     RowBox[{
      RowBox[{"FileNameTake", "/@", 
       RowBox[{"FileNames", "[", 
        RowBox[{"\"\<.*\>\"", ",", 
         RowBox[{"{", 
          RowBox[{"DirectoryName", "[", "installationDirectory", "]"}], 
          "}"}]}], "]"}]}], ",", "\"\<.git\>\""}], "]"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"Message", "[", 
     RowBox[{
      RowBox[{"Toolbox", "::", "gitrepository"}], ",", 
      "installationDirectory"}], "]"}], ",", "\[IndentingNewLine]", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"ChoiceDialog", "[", 
       RowBox[{"StringJoin", "[", 
        RowBox[{
        "\"\<Are you sure that you want to delete the following directory:\\n\
\>\"", ",", "installationDirectory", ",", 
         "\"\<\\nThis action cannot be undone.\>\""}], "]"}], "]"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"DeleteDirectory", "[", 
       RowBox[{"installationDirectory", ",", 
        RowBox[{"DeleteContents", "\[Rule]", "True"}]}], "]"}]}], 
     "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", "]"}], 
  ";"}]}], "Input",
 CellChangeTimes->{{3.628267132443421*^9, 3.628267135314639*^9}}]
}, Open  ]],

Cell[CellGroupData[{

Cell["\<\
Run the following code to deploy the Toolbox (for developers only!)\
\>", "Subsection"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"deploy", "[", "path_String", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"gitCommitHash", ",", "path2", ",", "configPath"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"gitCommitHash", "=", 
       RowBox[{
        RowBox[{"StringSplit", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"StringSplit", "[", 
            RowBox[{
             RowBox[{"Import", "[", 
              RowBox[{"\"\<.git/logs/HEAD\>\"", ",", "\"\<Text\>\""}], "]"}], 
             ",", "\"\<\\n\>\""}], "]"}], "[", 
           RowBox[{"[", 
            RowBox[{"-", "1"}], "]"}], "]"}], ",", "\"\< \>\""}], "]"}], "[", 
        
        RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"path2", "=", 
       RowBox[{"FileNameJoin", "[", 
        RowBox[{"{", 
         RowBox[{"path", ",", 
          RowBox[{
           RowBox[{"DateString", "[", 
            RowBox[{"{", 
             RowBox[{
             "\"\<Year\>\"", ",", "\"\<\>\"", ",", "\"\<Month\>\"", ",", 
              "\"\<\>\"", ",", "\"\<Day\>\""}], "}"}], "]"}], "<>", 
           "\"\<_\>\"", "<>", "\"\<MASS-Toolbox\>\"", "<>", "\"\<_\>\"", "<>", 
           RowBox[{"StringTake", "[", 
            RowBox[{"gitCommitHash", ",", 
             RowBox[{"{", 
              RowBox[{"1", ",", "8"}], "}"}]}], "]"}]}]}], "}"}], "]"}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"!", 
         RowBox[{"DirectoryQ", "[", "path2", "]"}]}], ",", 
        RowBox[{"CreateDirectory", "[", "path2", "]"}], ",", 
        RowBox[{
         RowBox[{"DeleteDirectory", "[", 
          RowBox[{"path2", ",", 
           RowBox[{"DeleteContents", "\[Rule]", "True"}]}], "]"}], ";", 
         RowBox[{"CreateDirectory", "[", "path2", "]"}], ";"}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"SetDirectory", "[", 
       RowBox[{"NotebookDirectory", "[", "]"}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Quiet", "[", 
       RowBox[{
        RowBox[{"DeleteDirectory", "[", 
         RowBox[{
          RowBox[{"FileNameJoin", "[", 
           RowBox[{"{", 
            RowBox[{"path2", ",", "\"\<Toolbox\>\""}], "}"}], "]"}], ",", 
          RowBox[{"DeleteContents", "\[Rule]", "True"}]}], "]"}], ",", 
        RowBox[{"{", 
         StyleBox[
          RowBox[{"DeleteDirectory", "::", "nodir"}], "MessageName"], "}"}]}],
        "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"CopyDirectory", "[", 
       RowBox[{"\"\<Toolbox\>\"", ",", 
        RowBox[{"FileNameJoin", "[", 
         RowBox[{"{", 
          RowBox[{"path2", ",", "\"\<Toolbox\>\""}], "}"}], "]"}]}], "]"}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"DeleteDirectory", "[", 
       RowBox[{
        RowBox[{"FileNameJoin", "[", 
         RowBox[{"{", 
          RowBox[{"path2", ",", "\"\<Toolbox/Documentation\>\""}], "}"}], 
         "]"}], ",", 
        RowBox[{"DeleteContents", "\[Rule]", "True"}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"CopyDirectory", "[", 
       RowBox[{"\"\<build/Toolbox/Documentation\>\"", ",", 
        RowBox[{"FileNameJoin", "[", 
         RowBox[{"{", 
          RowBox[{
          "path2", ",", "\"\<Toolbox\>\"", ",", "\"\<Documentation\>\""}], 
          "}"}], "]"}]}], "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"CopyFile", "[", 
       RowBox[{"\"\<LICENSE.txt\>\"", ",", 
        RowBox[{"FileNameJoin", "[", 
         RowBox[{"{", 
          RowBox[{"path2", ",", "\"\<LICENSE.txt\>\""}], "}"}], "]"}]}], 
       "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"CopyFile", "[", 
       RowBox[{"\"\<README.md\>\"", ",", 
        RowBox[{"FileNameJoin", "[", 
         RowBox[{"{", 
          RowBox[{"path2", ",", "\"\<README.md\>\""}], "}"}], "]"}]}], "]"}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{"Set", " ", "version", " ", "information", " ", "in", " ", 
        RowBox[{"Config", ".", "m"}]}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"configPath", "=", 
       RowBox[{"FileNameJoin", "[", 
        RowBox[{"{", 
         RowBox[{"path2", ",", "\"\<Toolbox/Config.m\>\""}], "}"}], "]"}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"Export", "[", 
       RowBox[{"configPath", ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Import", "[", 
          RowBox[{"configPath", ",", "\"\<Text\>\""}], "]"}], "<>", 
         "\"\<\\n\\n$Toolbox$Version = \\\"\>\"", "<>", "gitCommitHash", 
         "<>", "\"\<\\\"\>\""}], "\[IndentingNewLine]", ",", "\"\<Text\>\""}],
        "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Export", "[", 
       RowBox[{
        RowBox[{"FileNameJoin", "[", 
         RowBox[{"{", 
          RowBox[{"path2", ",", "\"\<Installer.nb\>\""}], "}"}], "]"}], ",", 
        RowBox[{"Notebook", "[", 
         RowBox[{
          RowBox[{"Import", "[", "\"\<Installer.nb\>\"", "]"}], "[", 
          RowBox[{"[", 
           RowBox[{"1", ",", 
            RowBox[{"1", ";;", "2"}]}], "]"}], "]"}], "]"}]}], "]"}], ";"}]}],
     "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"deploy", "[", "\"\</Users/niko/tmp/\>\"", "]"}]}], "Input",
 CellChangeTimes->{{3.581985596844112*^9, 3.581985649162616*^9}, {
