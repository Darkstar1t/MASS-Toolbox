Cell["Run the following code to uninstall the Toolbox", "Subsection"],

Cell["\<\
The following code will determine the location of the currently installed \
MASS Toolbox and uninstall it\
\>", "Text"],

Cell[BoxData[{
 RowBox[{"<<", "Toolbox`"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"Toolbox", "::", "gitrepository"}], "=", 
   "\"\<Package found at `1` is a GIT repository and not a regular \
installation of the Toolbox. It will not be removed.\>\""}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"SetDirectory", "[", 
   RowBox[{"NotebookDirectory", "[", "]"}], "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"installationDirectory", "=", 
   RowBox[{"FileNameJoin", "[", 
    RowBox[{
     RowBox[{"FileNameSplit", "[", 
      RowBox[{"DirectoryName", "[", 
       RowBox[{"FindFile", "[", "\"\<Toolbox`\>\"", "]"}], "]"}], "]"}], "[", 
     
     RowBox[{"[", 
      RowBox[{";;", 
       RowBox[{"-", "2"}]}], "]"}], "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{"MemberQ", "[", 
     RowBox[{
      RowBox[{"FileNameTake", "/@", 
       RowBox[{"FileNames", "[", 
        RowBox[{"\"\<.*\>\"", ",", 
         RowBox[{"{", 
          RowBox[{"DirectoryName", "[", "installationDirectory", "]"}], 
          "}"}]}], "]"}]}], ",", "\"\<.git\>\""}], "]"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"Message", "[", 
     RowBox[{
      RowBox[{"Toolbox", "::", "gitrepository"}], ",", 
      "installationDirectory"}], "]"}], ",", "\[IndentingNewLine]", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"ChoiceDialog", "[", 
       RowBox[{"StringJoin", "[", 
        RowBox[{
        "\"\<Are you sure that you want to delete the following directory:\\n\
\>\"", ",", "installationDirectory", ",", 
         "\"\<\\nThis action cannot be undone.\>\""}], "]"}], "]"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"DeleteDirectory", "[", 
       RowBox[{"installationDirectory", ",", 
        RowBox[{"DeleteContents", "\[Rule]", "True"}]}], "]"}]}], 
     "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"installationDirectory", "=", 
   RowBox[{"FileNameJoin", "[", 
    RowBox[{
     RowBox[{"FileNameSplit", "[", 
      RowBox[{"DirectoryName", "[", 
       RowBox[{"FindFile", "[", "\"\<AutomaticUnits`\>\"", "]"}], "]"}], 
      "]"}], "[", 
     RowBox[{"[", 
      RowBox[{";;", 
       RowBox[{"-", "2"}]}], "]"}], "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{"MemberQ", "[", 
     RowBox[{
      RowBox[{"FileNameTake", "/@", 
       RowBox[{"FileNames", "[", 
        RowBox[{"\"\<.*\>\"", ",", 
         RowBox[{"{", 
          RowBox[{"DirectoryName", "[", "installationDirectory", "]"}], 
          "}"}]}], "]"}]}], ",", "\"\<.git\>\""}], "]"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"Message", "[", 
     RowBox[{
      RowBox[{"Toolbox", "::", "gitrepository"}], ",", 
      "installationDirectory"}], "]"}], ",", "\[IndentingNewLine]", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"ChoiceDialog", "[", 
